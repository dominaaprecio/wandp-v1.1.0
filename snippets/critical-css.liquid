  }
  .wp-burger__line{
    width: 22px;
    height: 3px;
    background-color: #333334;
    display: block;
    margin: 4px auto;
    -webkit-transition: all 0.3s ease-in-out;
    -o-transition: all 0.3s ease-in-out;
