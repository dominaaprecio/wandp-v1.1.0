<style type="text/css">
  #sw-promo {
	/*background: url('https://cdn.shopify.com/s/files/1/1002/8980/files/wp-sw_banner_product.jpg?7790528007626955187') no-repeat;
  */background: url('https://cdn.shopify.com/s/files/1/1002/8980/files/wp-sw_banner_space.jpg?7790528007626955187');
    background-size: cover;
    background-position: center;
    height: 100px;
    /*margin-top: 50px;*/
    position: relative;
    display: flex;
    align-items: center;
    justify-content: center;
  }
  @media (min-width: 769px) {
    #sw-promo {
      height: 120px;
    }
  }
  #sw-promo::before {
    display: none;
    position: absolute;
    content: '';
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    -moz-box-shadow: rgba(0, 0, 0, 0.8) 5px 5px 55px inset;
    -webkit-box-shadow: rgba(0, 0, 0, 0.8) 5px 5px 55px inset;
    box-shadow: rgba(0, 0, 0, 0.8) 5px 5px 55px inset;
  }
  #star-wars-wp-logo {
    fill: #ffffff;
    width: 150px;
    height: auto;
    position: relative;
    margin: 0 auto;
/*     transform: perspective(300px) rotateX(25deg);
    transform-origin: 50% 100%; */
  }
  @media (min-width: 769px) {
    #star-wars-wp-logo {
      width: 180px;
    }
  }
  @media (min-width: 1024px) {
    #star-wars-wp-logo {
      width: 200px;
    }
  }
  .sw-promo-scroll-block {
    display: flex;
    align-items: center;
    justify-content: center;
    width: 100%;
    height: 100%;
    position: absolute;
    left: 0;
    top: 0;
    transition: all 0.3s ease;
    line-height: 1.3;
  }
  .sw-promo-scroll-block span {
    border-bottom: 3px solid #fff;
    transition: all 0.3s ease;
  }
  #sw-promo-zoom-out {
    animation: sw-zoom 12s ease-out infinite;
    animation-delay: 1s;
  }
  #sw-promo-scroll-up {
    animation: sw-scroll 12s linear infinite;
    animation-delay: 1s;
    transform: perspective(300px) rotateX(25deg);
    transform-origin: 50% 100%;
    color: #fff;
    font-size: 24px;
    opacity: 0;
    font-weight: 700;
  }
  @media (max-width: 768px) {
    #sw-promo-scroll-up {
      font-size: 18px;
    }
  }
  @media (max-width: 374px) {
    #sw-promo-scroll-up {
      font-size: 16px;
    }
  }
  @media (min-width: 769px) {
    #sw-promo-scroll-up {
      font-size: 28px;
    }
  }
  @keyframes sw-zoom {
    0% {
      opacity: 0;
      transform: scale(2);
    }
    4% {
      opacity: 1;
    }
    44% {
      opacity: 0;
      transform: scale(0.1);
    }
    100% {
      opacity: 0;
      transform: scale(2);
    }
  }
  @keyframes sw-scroll {
    0% {
      opacity: 0;
      top: 100%;
    }
    25% {
      opacity: 0.5;
      top: 50%;
    }
    50% {
      opacity: 1;
      top: 0;
    }
    75% {
      opacity: 0.5;
      top: -50%;
    }
    100% {
      opacity: 0;
      top: -100%;
    }
  }
  @media (min-width: 769px) {
    #star-wars-wp-logo {
      width: 260px;
  	  height: auto;
    }
  } 
